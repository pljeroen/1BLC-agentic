{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/1brc/run-manifest.schema.json",
  "title": "1BRC Run Manifest",
  "type": "object",
  "required": [
    "schema_version",
    "timestamp_utc",
    "input_file",
    "input_sha256",
    "jar_file",
    "jar_sha256",
    "output_file",
    "output_sha256",
    "stdout_file",
    "stdout_sha256",
    "stderr_file",
    "stderr_sha256",
    "command",
    "elapsed_ms",
    "time_to_beat_ms",
    "delta_vs_time_to_beat_ms",
    "delta_vs_time_to_beat_percent"
  ],
  "properties": {
    "schema_version": { "type": "string" },
    "timestamp_utc": { "type": "string" },
    "input_file": { "type": "string" },
    "input_sha256": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "jar_file": { "type": "string" },
    "jar_sha256": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "output_file": { "type": "string" },
    "output_sha256": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "stdout_file": { "type": "string" },
    "stdout_sha256": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "stderr_file": { "type": "string" },
    "stderr_sha256": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "command": { "type": "string" },
    "elapsed_ms": { "type": "number" },
    "phase_split_ms": { "type": "number" },
    "phase_process_merge_ms": { "type": "number" },
    "phase_format_ms": { "type": "number" },
    "phase_total_ms": { "type": "number" },
    "time_to_beat_ms": { "type": "number" },
    "delta_vs_time_to_beat_ms": { "type": "number" },
    "delta_vs_time_to_beat_percent": { "type": "number" }
  },
  "additionalProperties": true
}
